{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix || true",
    "typecheck": "tsc --noEmit",
    "services:api": "node services/api-gateway/server.js",
    "services:ai": "node services/ai-engine/server.js",
    "services:all": "npm-run-all --parallel services:api services:ai",
    "export:docs": "npx md-to-pdf docs/*.md --output-dir dist/docs",
    "perf:audit": "lighthouse http://localhost:9003 --output=html --output-path=./lighthouse-report.html"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@firebase/app-types": "^0.9.1",
    "@genkit-ai/firebase": "latest",
    "@google/generative-ai": "^0.11.3",
    "@mui/material": "^5.15.19",
    "firebase": "^12.3.0",
    "firebase-admin": "^12.1.0",
    "firebase-functions": "^5.0.1",
    "firebase-tools": "^13.11.1",
    "framer-motion": "^11.2.6",
    "genkit": "latest",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "next": "^15.0.0",
    "next-i18next": "^15.3.0",
    "react": "^18",
    "react-dom": "^18",
    "react-i18next": "^14.1.2",
    "react-icons": "^5.2.1",
    "react-intersection-observer": "^9.10.2",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "eslint": "^8",
    "eslint-config-next": "^15.0.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  }
}